extends base

block content
  .input-group(style="padding: 20px")
    button.form__input.bg-red#goBack Go Back 
    button.form__input.bg-green#export Export
  .bookings-container 
    .button-group 
      select(class="select-book")
        option(value="clientName") search by Client Name 
        option(value="clientEmail") search by Client Email 
        option(value="hallname") search by Hall Name 
        option(value="bookedFrom") Search by Date (mm/dd/yyyy)
 
 
      input(type="text" placeholder="Enter search Details" class="input-filter")
      button(class="btn-submit") Submit
      button(onclick="document.location.href='/csv' ") Download csv 
      button(onclick="document.location.href='/bin' ") bin 
      button(onclick="document.location.href='/calender' ") calender
      button.rangeBtn search range

    .range
      .range-one 
        label(for="from") From 
        input(type="date" name="from" class="range_from")
      .range-two 
        label(for="to") To
        input(type="date" name="to" class="range_to")
      button(id="rangeB") Search
      
    .table-form
      table.tab
        th Client Name
        th Client Email
        th Client Phone Number
        th Hallname
        th Attendance
        th Event
        th Booked From
        th Booked To     
        th Payment     
        th Total     
        th Session     
        th Discount     
        th Delete    
        th Edit    
